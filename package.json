{
  "name": "palette-search",
  "displayName": "Palette Search",
  "description": "A vscode extension that opens the browser for what you search ",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/techmaved/palette-search"
  },
  "license": "Public domain license",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:palette-search.search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "palette-search.search",
        "title": "Palette Search"
      }
    ],
    "configuration": [
      {
        "id": "searchEngineConf",
        "title": "Search engine",
        "properties": {
          "palette-search.searchEngine": {
            "type": "string",
            "enum": [
              "DuckDuckGo",
              "Bing"
            ],
            "default": "Google",
            "description": "Choose the search engine you want to query when using the extension.",
            "scope": "window"
          }
        }
      },
      {
        "id": "searchEngineConfFree",
        "title": "Search engine - own",
        "properties": {
          "palette-search.searchEngineFree": {
            "type": "string",
            "input": true,
            "default": "",
            "description": "Paste you own search engine here: Search strings or unsupported browsers are the keywords you need for the url. Only full urls work.",
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@vscode/vsce": "^2.16.0"
  }
}
